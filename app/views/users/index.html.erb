<h1>All users</h1>
<hr>
<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.first_name + " " + user.last_name, user %>
      | <%= user.phone %>
      <% if current_user.admin? && !current_user?(user) %>
      | <%= link_to "Delete", user, method: :delete, data: {confirm: "You sure?" } %>
      <% end %>
    </li>
  <% end %>
</ul>